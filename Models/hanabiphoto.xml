<?xml version="1.0"?>

<PropertyList>
	<path>hanabiphoto.ac</path>

		  <animation>
		    <type>billboard</type> 
                    <object-name>Plane2</object-name>
                    <spherical type="bool">true</spherical>
                  </animation>
		  <animation>
		    <type>material</type> 
                    <object-name>Plane2</object-name>
		    <emission>	
		      <red>1</red>	
		      <green>1</green>	
		      <blue>1</blue>
		    </emission>
                     <texture>hanabiphoto.png</texture>
		  </animation>

   <!-- Transparent Surfaces for Rembrandt rendering -->
   <effect>
       <inherits-from>Effects/model-transparent</inherits-from>
   　  <condition>
      　 <property>sim/rendering/rembrandt/enabled</property>
   　  </condition>
         <object-name>Plane2</object-name>
   </effect>

